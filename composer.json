{
  "name": "wpwhales/plugin-base",
  "description": "Illuminate packages are customized to for the base for a plugin boilerplate known as Whalevel",
  "keywords": [
    "wordpress",
    "plugin",
    "boilerplate",
    "wordpress-larvel",
    "whalevel"
  ],
  "license": "MIT",
  "homepage": "https://wpwhales.io",
  "support": {
    "issues": "https://github.com/wpwhales/plugin-base/issues",
    "source": "https://github.com/wpwhales/plugin-base"
  },
  "authors": [
    {
      "name": "WPWhales",
      "email": "hello@wpwhales.io"
    }
  ],
  "require": {
    "php": "^8.2",
    "wpwhales/container": "dev-master",
    "wpwhales/config": "dev-master"
  },
  "suggest": {
    "nyholm/psr7": "Required to use PSR-7 bridging features (^1.2).",
    "symfony/psr-http-message-bridge": "Required to use PSR-7 bridging features (^2.0)."
  },
  "autoload": {
    "psr-4": {
      "WPWhales\\Plugin\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "9.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
